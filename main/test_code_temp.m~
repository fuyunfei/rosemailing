clear all; close all ; 
a=1:25;
a=reshape(a,[5,5]); 
p=[2;2];
[f1,S,q] = perform_fast_marching(a, p);
clf;
hold on;
surf(f1);
h = plot3(p(2,:), p(1,:), f1(p(1,:),p(2,:)), 'r.');
set(h, 'MarkerSize',30);
%colormap(gray(256));
shading interp;
axis('equal');
view(110,45);
axis('off');
camlight('headlight')                                                                                                                                            